palindrome <- function(x) {
isPalindrome <- TRUE
midpoint <- floor(nchar(x)/2)
for (i in 1:midpoint){
idx <- nchar(x) - (i -1)
if (!(x[i] == x[idx])){
isPalindrome <- FALSE
}
}
}
